.row
  .col-md-5.form.left-spacing
    .well.well-lg.register-box
      h1 Update Profile
      form.register-button-space(ng-submit="updateProfile(friendCode)")
        label Friend Code
        input.form-control.form-space(type='text', placeholder='Enter Friend Code', ng-model='friendCode')
        button.btn.btn-info.btn-block.add-button-space(type="submit") Update Profile
